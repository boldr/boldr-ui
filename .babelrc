{
  "env": {
    "story": {
      "presets": [["env", { "modules": "commonjs" }], "react"],
      "plugins": [
        "transform-class-properties",
        [
          "transform-object-rest-spread",
          {
            "useBuiltIns": true
          }
        ],
        "transform-flow-strip-types",
        ["babel-plugin-react-docgen", { "DOC_GEN_COLLECTION_NAME": "BOLDR_UI_DOCS" }]
      ]
    },
    "development": {
      "sourceMaps": "both",
      "presets": ["./internal/babel/env-preset", "react"],
      "plugins": [
        "transform-class-properties",
        [
          "transform-object-rest-spread",
          {
            "useBuiltIns": true
          }
        ],
        "transform-flow-strip-types"
      ]
    },
    "test": {
      "sourceMaps": "both",
      "presets": ["./internal/babel/env-preset", "react"],
      "plugins": [
        "transform-class-properties",
        [
          "transform-object-rest-spread",
          {
            "useBuiltIns": true
          }
        ],
        "transform-flow-strip-types"
      ]
    },
    "sb:production": {
      "compact": true,
      "comments": false,
      "presets": [
        [
          "env",
          {
            "targets": {
              "ie": 11,
              "edge": 14,
              "firefox": 45,
              "chrome": 49,
              "safari": 10,
              "node": "6.11"
            },
            "modules": "commonjs"
          }
        ],
        "react"
      ],
      "plugins": [
        "transform-class-properties",
        ["transform-object-rest-spread", {
          "useBuiltIns": true
        }],
        "transform-react-constant-elements",

        [
          "transform-react-remove-prop-types",
          {
            "mode": "wrap",
            "ignoreFilenames": ["node_modules"],
            "plugins": ["babel-plugin-transform-flow-strip-types"]
          }
        ]
      ]
    },
    "production": {
      "compact": true,
      "comments": false,
      "presets": ["./internal/babel/env-preset", "react"],
      "plugins": [
        "transform-class-properties",
        ["transform-object-rest-spread", {
          "useBuiltIns": true
        }],
        "transform-react-constant-elements",

        [
          "transform-react-remove-prop-types",
          {
            "mode": "wrap",
            "ignoreFilenames": ["node_modules"],
            "plugins": ["babel-plugin-transform-flow-strip-types"]
          }
        ]
      ]
    }
  }
}
