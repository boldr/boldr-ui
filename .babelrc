{
  "env": {
    "story": {
      "presets": [["env", { "modules": "commonjs" }], "react"],
        "plugins": [
          "transform-class-properties",
          "transform-object-rest-spread",
          "transform-flow-strip-types"
        ]
    },
    "development": {
      "sourceMaps": "both",
      "presets": ["./internal/babel/env-preset", "react"],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-flow-strip-types"
      ]
    },
    "test": {
      "sourceMaps": "both",
      "presets": ["./internal/babel/env-preset", "react"],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-flow-strip-types"
      ]
    },
    "sb:production": {
      "compact": true,
      "comments": false,
      "presets": [["env", {
        "targets": {
          "ie": 11,
          "edge": 14,
          "firefox": 45,
          "chrome": 49,
          "safari": 10,
          "node": "6.11"
        },
        "modules":  "commonjs"
      }], "react"],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-react-constant-elements",

        ["transform-react-remove-prop-types", {
          "mode": "wrap",
          "plugins": [
            "babel-plugin-transform-flow-strip-types"
          ]
        }]
      ]
    },
    "production": {
      "compact": true,
      "comments": false,
      "presets": ["./internal/babel/env-preset", "react"],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-react-constant-elements",

        ["transform-react-remove-prop-types", {
          "mode": "wrap",
          "plugins": [
            "babel-plugin-transform-flow-strip-types"
          ]
        }]
      ]
    }
  }
}
