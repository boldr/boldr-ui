  "files": [
    "dist/",
    "lib/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "keywords": [
    "boldr",
    "react"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote=true --jsx-bracket-same-line=false --trailing-comma=all --write \"src/**/*.js\"",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "peerDependencies": {
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "react-transition-group": "^1.1.1",
    "styled-components": "^1.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boldr/boldr-ui.git"
  },
  "scripts": {
    "build": "NODE_ENV=production npm run clean && npm run build:umd && npm run build:es && npm run build:sass",
    "build:es": "NODE_ENV=production BABEL_ENV=es babel --ignore=__tests__,__mocks__,src/**/*.test.js ./src -d ./lib",
    "build:sass": "npm run sass && cp -r src/styles lib && cp dist/boldrui.css lib",
    "build:test": "echo \"nothing\" && exit 0",
    "build:umd": "webpack --config internal/webpack/webpack.prod.config.js --progress --colors",
    "changelog": "conventional-changelog -p atom -i CHANGELOG.md",
    "clean": "rimraf dist && rimraf lib",
    "format": "prettier --single-quote=true --jsx-bracket-same-line=false --trailing-comma=all --write \"src/**/*.js\"",
    "precommit": "lint-staged",
    "sass": "node-sass --output-style compressed src/styles/boldrui.scss -o dist",
    "start": "cross-env NODE_ENV=development BABEL_ENV=es UV_THREADPOOL_SIZE=100 node ./server",
    "test": "NODE_ENV=test jest -u --config=internal/jest/jest.json",
    "test:ci": "NODE_ENV=test jest --config=internal/jest/jest.json --forceExit"
  }
}
