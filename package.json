{
  "name": "@boldr/ui",
  "private": true,
  "author": "Steven Truesdell",
  "version": "0.1.0-beta.1",
  "description": "UI components for Boldr",
  "main": "./src/index.js",
  "keywords": [
    "react",
    "boldr",
    "react-ui",
    "react-component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boldr/boldr-ui.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boldr/boldr-ui/issues"
  },
  "homepage": "https://boldr.io/",
  "scripts": {
    "deploy": "cd docs && yarn build",
    "build": "npm run build:es2015 && npm run build:es2015modules && npm run build:copy-files && npm run build:umd:dev && npm run build:umd:min",
    "build:es2015": "cross-env NODE_ENV=production babel ./src --ignore *.spec.js --out-dir ./build",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./build/index.es.js",
    "build:copy-files": "babel-node ./internal/scripts/copy-files.js",
    "build:umd:dev": "webpack",
    "build:umd:min": "cross-env NODE_ENV=production webpack",
    "build:docs": "babel-node ./internal/scripts/build-api-docs.js",
    "build:sass": "npm run sass && cp -r src/styles build && cp build/dist/boldrui.css build",
    "prettier": "find . -name \"*.js\" | grep -v -f .eslintignore | xargs prettier --write --jsx-bracket-same-line=false --single-quote --trailing-comma all --print-width 100",
    "clean": "npm run clean:build",
    "clean:docs": "rimraf docs/api/*",
    "clean:build": "rimraf build",
    "lint": "eslint . --cache && echo \"eslint: no lint errors\"",
    "prebuild": "npm run clean:build",
    "sass": "node-sass --output-style compressed src/styles/boldrui.scss -o build",
    "start": "cd docs && npm start",
    "flow": "flow",
    "test": "NODE_ENV=test jest",
    "test:ci": "NODE_ENV=test jest",
    "publish": "npm run build && cd build && npm publish --tag next"
  },
  "peerDependencies": {
    "react": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "react-transition-group": "^1.2.0",
    "styled-components": "^2.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "dom-helpers": "^3.2.1",
    "hoist-non-react-statics": "^1.2.0",
    "lodash": "^4.17.4",
    "keycode": "^2.1.9",
    "object-assign": "^4.1.1",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react-modal": "^2.0.6",
    "react-visibility-sensor": "^3.10.0",
    "styled-theme": "^0.3.3",
    "styled-tools": "^0.1.4",
    "uuid": "^3.1.0",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-flow-react-proptypes": "^3.4.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-ramda": "^1.2.0",
    "babel-plugin-react-remove-properties": "^0.2.5",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.1",
    "cross-env": "^5.0.1",
    "doctrine": "^2.0.0",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.0.0",
    "eslint-config-boldr": "^0.10.3",
    "eventsource-polyfill": "^0.9.6",
    "flow-bin": "^0.48.0",
    "flow-copy-source": "^1.1.0",
    "flow-typed": "^2.1.2",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "html-looks-like": "^1.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.2.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^2.11.0",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.1",
    "prettier": "^1.4.4",
    "random-words": "0.0.1",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-addons-perf": "^15.4.2",
    "react-docgen": "^3.0.0-beta4",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.6.1",
    "recast": "^0.12.5",
    "recursive-readdir-sync": "^1.0.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.5",
    "stylelint": "^7.11.1",
    "stylelint-config-boldr": "^0.1.1",
    "url-loader": "^0.5.9",
    "webfontloader": "^1.6.28",
    "webpack": "^3.0.0"
  },
  "jest": {
    "timers": "fake",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/(lib|internal|build|docs|dist|packages|flow)/",
      "/flow-typed/",
      "/__fixtures__/",
      "/node_modules/",
      "<rootDir>/src/styles/"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/internal/jest/transform.js"
    },
    "testRegex": ".*.test\\.js",
    "setupTestFrameworkScriptFile": "<rootDir>/internal/jest/setup.js",
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(lib|internal|build|docs|dist|packages|flow)/",
      "__snapshots__",
      "/styles/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "/docs/",
      "/flow/",
      "/styles/"
    ],
    "coverageReporters": [
      "json"
    ]
  }
}
