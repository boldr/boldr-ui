{
  "private": "true",
  "name": "material-ui-icons-builder",
  "version": "1.0.0-alpha.3",
  "description": "Material Design Svg Icons converted to Material-UI React components.",
  "author": "Material-UI Team",
  "license": "MIT",
  "homepage": "http://material-ui.com/",
  "bugs": {
    "url": "https://github.com/callemall/material-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "callemall/material-ui"
  },
  "keywords": [
    "Material",
    "Design",
    "Material-UI",
    "Icons",
    "React",
    "react-component"
  ],
  "scripts": {
    "build": "npm run build:icons && npm run build:babel && npm run build:copy-files",
    "build:babel": "babel --presets es2015,react -d ./build ./src",
    "build:copy-files": "babel-node ./copy-files.js",
    "build:icons": "babel-node ./build.js --output-dir ./src --svg-dir ./node_modules/material-design-icons --glob '/**/production/*_24px.svg' --renameFilter ./filters/rename/material-design-icons.js",
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "test": "mocha",
    "test:watch": "mocha -w"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.0.0",
    "fs-extra": "^2.0.0",
    "glob": "^7.0.0",
    "lodash": "^4.0.0",
    "material-design-icons": "^3.0.0",
    "mkdirp": "^0.5.0",
    "mocha": "^3.3.0",
    "mustache": "^2.0.0",
    "recursive-readdir-sync": "^1.0.6",
    "rimraf": "^2.0.0",
    "temp": "^0.8.3",
    "yargs": "^7.1.0"
  }
}
